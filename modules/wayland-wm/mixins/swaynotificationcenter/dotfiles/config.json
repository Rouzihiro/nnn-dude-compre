{
  "$schema": "/etc/xdg/swaync/configSchema.json",
  "positionX": "right",
  "positionY": "bottom",
  "layer": "overlay",
  "control-center-layer": "overlay",
  "cssPriority": "user",
  "control-center-positionX": "right",
  "notification-icon-size": 56,
  "notification-body-image-height": 56,
  "notification-body-image-width": 56,
  "timeout": 3,
  "timeout-low": 3,
  "timeout-critical": 0,
  "fit-to-screen": true,
  "control-center-width": 324,
  "notification-window-width": 317,
  "keyboard-shortcuts": true,
  "image-visibility": "always",
  "transition-time": 250,
  "hide-on-clear": false,
  "hide-on-action": true,
  "script-fail-notify": true,
  "scripts": {
    "play-sound": {
      "exec": "play-notification-sound",
      "urgency": "Critical"
    }
  },
  "widgets": ["title", "dnd", "notifications"]
}

# Focus desktop
fn + cmd - 1 : yabai -m space --focus 1
fn + cmd - 2 : yabai -m space --focus 2
fn + cmd - 3 : yabai -m space --focus 3
fn + cmd - 4 : yabai -m space --focus 4

# Send window to desktop
cmd + shift - 1 : yabai -m window --space 1
cmd + shift - 2 : yabai -m window --space 2
cmd + shift - 3 : yabai -m window --space 3
cmd + shift - 4 : yabai -m window --space 4

# Change layout of desktop
alt + cmd - b : yabai -m space --layout bsp
alt + cmd - m : yabai -m space --layout stack
alt + cmd - f : yabai -m space --layout float

# fn + cmd - f : yabai -m window --toggle zoom-fullscreen

# Apps
cmd - return : /Applications/kitty.app/Contents/MacOS/kitty --single-instance -d ~

Host *
  AddKeysToAgent yes
  IgnoreUnknown UseKeychain
  UseKeychain yes

Host github.com-boc
  HostName github.com
  User git
  IdentitiesOnly yes
  IdentityFile ~/.ssh/id_rsa_boc

Host bitbucket.org-boc
  HostName bitbucket.org
  User git
  IdentitiesOnly yes
  IdentityFile ~/.ssh/id_rsa_boc

Host github.com
  HostName github.com
  User git
  IdentitiesOnly yes
  IdentityFile ~/.ssh/id_rsa

